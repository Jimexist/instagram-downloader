{
  "manifest_version": 2,
  "name": "Instagram Downloader",
  "description": "Download Instagram page to a URL list",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.instagram.com/*"
      ],
      "css": [],
      "js": [
        "dist/content.js"
      ]
    }
  ]
}
